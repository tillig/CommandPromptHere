; {1} PowerToy
;
; Generated by Command Prompt Here Generator - Travis Illig
; http://www.paraesthesia.com
;
; Template adapted from original "CMD Prompt Here" PowerToy
; and Visual Studio prompt adaptations from Scott Hanselman
; http://www.computerzen.com
;

[version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = CurrentInstaller.Files.Inf
AddReg    = CurrentInstaller.Reg.Add

[DefaultUnInstall]
DelFiles  = CurrentInstaller.Files.Inf
DelReg    = CurrentInstaller.Reg.Add,CurrentInstaller.Reg.Remove

[SourceDisksNames]
1 = "{1} Here"

[SourceDisksFiles]
{0}.inf = 1

[DestinationDirs]
CurrentInstaller.Files.Inf = 17

[CurrentInstaller.Files.Inf]
{0}.inf

[CurrentInstaller.Reg.Add]
HKLM,%UDHERE%
HKLM,%UDHERE%,DisplayName,,"%CommandPromptName%"
HKLM,%UDHERE%,UninstallString,,"rundll32.exe syssetup.dll,SetupInfObjectInstallAction DefaultUninstall 132 %17%\{0}.inf"
HKCR,Directory\Shell\{0},,,"%CommandPromptAccel%"
HKCR,Directory\Shell\{0}\command,,,"{3}"
HKCR,Drive\Shell\{0},,,"%CommandPromptAccel%"
HKCR,Drive\Shell\{0}\command,,,"{3}"

[CurrentInstaller.Reg.Remove]
HKLM,%UDHERE%
HKCR,Directory\Shell\{0}
HKCR,Drive\Shell\{0}

[Strings]
CommandPromptName="{1} Here PowerToy"
CommandPromptAccel="{2}"
UDHERE="Software\Microsoft\Windows\CurrentVersion\Uninstall\{0}"</value>
    <comment>{0} is Id; {1} is Name; {2} is Accelerator; {3} is CommandLine</comment>
